{
  "name": "docco-husky",
  "description": "Generated static project documentation primarily for node.js projects. A fork of Docco",
  "keywords": [
    "documentation",
    "docs",
    "generator",
    "coffeescript"
  ],
  "author": "Mike Brevoort <mike@brevoort.com>",
  "version": "0.3.8",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/mit-license.php"
    }
  ],
  "contributors": [
    "Mike Brevoort <mike@brevoort.com>",
    "Jeremy Ashkenas"
  ],
  "engines": {
    "node": ">=0.4.0"
  },
  "bugs": {
    "url": "https://github.com/mbrevoort/docco-husky/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mbrevoort/docco-husky.git"
  },
  "dependencies": {
    "dox": "https://github.com/royriojas/dox/tarball/master",
    "jade": "0.17.0",
    "gravatar": "1.0.3",
    "underscore": "1.2.x",
    "escape-html": "~1.0.1",
    "marked": "~0.3.1",
    "glob-expand": "0.0.2"
  },
  "devDependencies" : {
    "coffee-script": "latest"
  },
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib/docco",
  "bin": {
    "docco-husky": "./bin/generate"
  },
  "scripts": {},
  "optionalDependencies": {},
  "docco_husky": {
    "content_dir": "md",
    "output_dir": "./documentation",
    "project_name": "docco-husky",
    "show_timestamp": true
  }
}
